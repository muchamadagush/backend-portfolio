{
  "name": "backend-portfolio",
  "version": "1.0.0",
  "description": "backend for portfolio app",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "type-check": "tsc --noEmit",
    "before-build": "npm run clean && yarn && npm run type-check",
    "build": "npm run before-build && tsc -p .",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:fresh": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npx sequelize-cli db:create && npm run db:migrate && npm run db:seed"
  },
  "repository": "https://github.com/muchamadagush/backend-portfolio",
  "author": "Muchamad Agus Hermawan",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.5",
    "sequelize-cli": "^6.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.12.13",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "import": "^0.0.6",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.x"
  }
}
